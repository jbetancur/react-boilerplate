{
  "name": "zero-generator",
  "version": "0.7.5",
  "description": "zero-generator is nothing fancy. Just a stupidly simple cli for generating code projects",
  "main": "dist/index.js",
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "start": "ts-node src/index.ts",
    "build": "tsc && shx rm -rf dist/templates && shx cp -r src/templates dist"
  },
  "bin": {
    "zero": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbetancur/zero-generator.git"
  },
  "keywords": [
    "react",
    "node",
    "boilerplate",
    "starter",
    "styled-components",
    "typescript",
    "jest"
  ],
  "author": "John Betancur",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jbetancur/zero-generator/issues"
  },
  "homepage": "https://github.com/jbetancur/zero-generator#readme",
  "devDependencies": {
    "@types/ejs": "^3.0.4",
    "@types/inquirer": "^7.3.0",
    "@types/node": "^14.0.27",
    "@types/shelljs": "^0.8.8",
    "@types/yargs": "^15.0.5",
    "shx": "^0.3.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "ejs": "^3.1.3",
    "inquirer": "^7.3.3",
    "shelljs": "^0.8.4",
    "yargs": "^15.4.1"
  }
}
